#!/command/with-contenv bash

AGGREGATE_CSV_FILE=${AGGREGATE_CSV_FILE:-"aggregate.csv"}
AGGREGATE_JSON_FILE=${AGGREGATE_JSON_FILE:-"aggregate.json"}
CONFIG_FILE=${CONFIG_FILE:-"parsedmarc.conf"}
CONFIG_PATH=${CONFIG_PATH:-"/config/"}
DATA_PATH=${DATA_PATH:-"/data/"}
ENABLE_IMAP=${ENABLE_IMAP:-"TRUE"}
FORENSIC_CSV_FILE=${FORENSIC_CSV_FILE:-"forensic.csv"}
FORENSIC_JSON_FILE=${FORENSIC_JSON_FILE:-"forensic.json"}
INPUT_PATH=${INPUT_PATH:-"${INPUT_PATH}/input/"}
LOG_FILE=${LOG_FILE:-"parsedmarc.log"}
LOG_PATH=${LOG_PATH:-"/logs/"}
MAILBOX_ARCHIVE_FOLDER=${MAILBOX_ARCHIVE_FOLDER:-"Archive"}
MAILBOX_BATCH_SIZE=${MAILBOX_BATCH_SIZE:-"10"}
MAILBOX_CHECK_INTERVAL=${MAILBOX_CHECK_INTERVAL:-"30"}
MAILBOX_DELETE_AFTER_PROCESSING=${MAILBOX_DELETE_AFTER_PROCESSING:-"TRUE"}
MAILBOX_REPORTS_FOLDER=${MAILBOX_REPORTS_FOLDER:-"INBOX"}
MAILBOX_TEST_MODE=${MAILBOX_TEST_MODE:-"FALSE"}
MAILBOX_WATCH=${MAILBOX_WATCH:-"TRUE"}
NAMESERVERS=${NAMESERVERS:-"1.1.1.1 1.0.0.1"}
OUTPUT_PATH=${OUTPUT_PATH:-"${DATA_PATH}/output/"}
REPORTS_SAVE_AGGREGATE=${REPORTS_SAVE_AGGREGATE:-"FALSE"}
REPORTS_SAVE_FORENSIC=${REPORTS_SAVE_FORENSIC:-"FALSE"}
SETUP_MODE=${SETUP_MODE:-"AUTO"}
TIMEOUT_DNS=${TIMEOUT_DNS:-"2.0"}